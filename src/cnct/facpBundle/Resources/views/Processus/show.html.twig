{% extends "cnctfacpBundle::layout.html.twig" %}

{% block title %}
	Administration - {{ parent() }}
{% endblock %}
{% block menu %}
        <h1><font color="#800080">Administration</font></h1>
{% endblock %}
{% block facp_body %}
<h1>Processus</h1>

<table class="table table-striped well" >
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Code_pro</th>
            <td>{{ entity.codepro }}</td>
        </tr>
        <tr>
            <th>Libelle</th>
            <td>{{ entity.libelle }}</td>
        </tr>
        <tr>
            <th>Pilote</th>
            <td>{{ entity.pilote }}</td>
        </tr>
        <tr>
            <th>Interime</th>
            <td>{{ entity.interime }}</td>
        </tr>
    </tbody>
</table>

<p class="well">
		<a href="{{ path('processus') }}" class="btn">
			<i class="icon-chevron-left"></i>
			Retour Ã  la liste
		</a>
		<a href="{{ path('processus_edit', { 'id': entity.id }) }}" class="btn">
			<i class="icon-edit"></i>
			Modifier le processus
		</a>
		
                <form action="{{ path('processus_delete', { 'id': entity.id }) }}" method="post">
                    {{ form_widget(delete_form) }}
                    <button type="submit">Delete</button>
                </form>
</p>

{% endblock %}
