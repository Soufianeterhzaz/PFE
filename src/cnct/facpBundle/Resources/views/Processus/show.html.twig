<h1>Processus</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Code_pro</th>
            <td>{{ entity.codepro }}</td>
        </tr>
        <tr>
            <th>Libelle</th>
            <td>{{ entity.libelle }}</td>
        </tr>
        <tr>
            <th>Pilote</th>
            <td>{{ entity.pilote }}</td>
        </tr>
        <tr>
            <th>Interime</th>
            <td>{{ entity.interime }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('processus') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('processus_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('processus_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
