<h1>Utilisateur</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Matricule</th>
            <td>{{ entity.matricule }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ entity.prenom }}</td>
        </tr>
        <tr>
            <th>Adresse_ip</th>
            <td>{{ entity.adresseip }}</td>
        </tr>
        <tr>
            <th>Categorie</th>
            <td>{{ entity.categorie }}</td>
        </tr>
        <tr>
            <th>Etat</th>
            <td>{{ entity.etat }}</td>
        </tr>
        <tr>
            <th>Grade</th>
            <td>{{ entity.grade }}</td>
        </tr>
        <tr>
            <th>Processus</th>
            <td>{{ entity.processus }}</td>
        </tr>
        <tr>
            <th>Direction</th>
            <td>{{ entity.direction }}</td>
        </tr>
        <tr>
            <th>Sous_direction</th>
            <td>{{ entity.sousdirection }}</td>
        </tr>
        <tr>
            <th>Privilege</th>
            <td>{{ entity.privilege }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('utilisateur') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('utilisateur_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('utilisateur_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
